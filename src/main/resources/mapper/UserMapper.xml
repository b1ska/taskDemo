<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.UserMapper">
    <select id="findUser" resultType="com.example.demo.model.User">
        SELECT *
        FROM public."Users"
    </select>

    <insert id="addNewUser">
        INSERT INTO "Users" ("userId", "userName", "userSex", "userYearOfBirthDay", "userEmail")
        VALUES (#{userId}, #{userName}, #{userSex}, #{userYearOfBirthDay}, #{userEmail})
    </insert>

</mapper>